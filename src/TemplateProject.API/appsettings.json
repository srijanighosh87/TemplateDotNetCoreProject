{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=test_database;Username=postgres;Password=postgres"
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/logs.json",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1000000,
          "retainedFileTimeLimit": "365.00:00:00",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "shared": true
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:u} {Level}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "SchemaDirectory": "schemas",
  "ConversionExpirationPeriod": "31.00:00:00"
}
